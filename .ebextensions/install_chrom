commands:
    command1:
        command: "wget https://dl.google.com/linux/direct/google-chrome-stable_current_x86_64.rpm"
    command2:
        command: "sudo yum install ./google-chrome-stable_current_x86_64.rpm"
    command3:
        command: "sudo ln -s /usr/bin/google-chrome-stable /usr/bin/chromium"
    
